{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Initialize Git Repository",
			"type": "shell",
			"command": "git init"
		},
		{
			"label": "Add all files to git",
			"type": "shell",
			"command": "git add ."
		},
		{
			"label": "Create initial commit",
			"type": "shell",
			"command": "git commit -m \"Initial commit\""
		},
		{
			"label": "Dry run GitHub repo creation",
			"type": "shell",
			"command": "chmod +x scripts/create_github_repo.sh && USERNAME=SoundSliced REPO=mywebview scripts/create_github_repo.sh --dry-run"
		},
		{
			"label": "Check git remotes",
			"type": "shell",
			"command": "git remote -v"
		},
		{
			"label": "Create GitHub repo",
			"type": "shell",
			"command": "USERNAME=SoundSliced REPO=mywebview scripts/create_github_repo.sh"
		},
		{
			"label": "Check git status",
			"type": "shell",
			"command": "git status"
		},
		{
			"label": "Check git remotes",
			"type": "shell",
			"command": "git remote -v"
		},
		{
			"label": "Push to GitHub",
			"type": "shell",
			"command": "git push -u origin main"
		},
		{
			"label": "Add remote origin",
			"type": "shell",
			"command": "git remote add origin https://github.com/SoundSliced/mywebview.git"
		},
		{
			"label": "Push to GitHub",
			"type": "shell",
			"command": "git push -u origin main"
		},
		{
			"label": "Pull from GitHub",
			"type": "shell",
			"command": "git pull origin main --allow-unrelated-histories"
		},
		{
			"label": "Fetch from origin",
			"type": "shell",
			"command": "git fetch origin"
		},
		{
			"label": "Pull from GitHub",
			"type": "shell",
			"command": "git pull --allow-unrelated-histories"
		},
		{
			"label": "Check branches",
			"type": "shell",
			"command": "git branch -a"
		},
		{
			"label": "Force push to GitHub",
			"type": "shell",
			"command": "git push --force -u origin main"
		},
		{
			"label": "Dry Run Publish",
			"type": "shell",
			"command": "flutter pub publish --dry-run"
		},
		{
			"label": "Flutter Analyze",
			"type": "shell",
			"command": "flutter analyze"
		},
		{
			"label": "Flutter Test",
			"type": "shell",
			"command": "flutter test"
		},
		{
			"label": "Create GitHub Repo Dry Run",
			"type": "shell",
			"command": "bash scripts/create_github_repo.sh --dry-run"
		},
		{
			"label": "Run MyWebView Test",
			"type": "shell",
			"command": "flutter test test/mywebview_test.dart"
		},
		{
			"label": "Run MyWebView Test",
			"type": "shell",
			"command": "flutter test test/mywebview_test.dart"
		},
		{
			"label": "Flutter Test",
			"type": "shell",
			"command": "flutter test"
		},
		{
			"label": "Run MyWebView Test",
			"type": "shell",
			"command": "flutter test test/mywebview_test.dart"
		},
		{
			"label": "Run MyWebView Test",
			"type": "shell",
			"command": "flutter test test/mywebview_test.dart"
		},
		{
			"label": "Run Release and Publish Script",
			"type": "shell",
			"command": "bash scripts/release_publish.sh"
		},
		{
			"label": "Flutter Web (Fixed Port 5000)",
			"type": "shell",
			"command": "flutter run -d chrome --web-port=5000",
			"isBackground": true,
			"problemMatcher": {
				"pattern": {
					"regexp": "^.*$",
					"file": 1,
					"location": 2,
					"message": 3
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": "^Launching lib/main.dart.*",
					"endsPattern": "^Application running on.*"
				}
			}
		},
		{
			"label": "Cleanup Documentation",
			"type": "shell",
			"command": "rm AUTO_DETECTION_CHANGES.md AUTO_DETECTION_COMPLETE.md AUTO_DETECTION_FEATURE.md AUTO_DETECTION_FIX.md BEFORE_AND_AFTER.md CHANGES_SUMMARY.md CONNECTIVITY_GUIDE.md CORS_PROXY_README.md FEATURE_ENHANCEMENT_PROPOSAL.md IMPLEMENTATION_SUMMARY.md PLATFORM_VERIFICATION_REPORT.md POINTER_INTERCEPTION.md QUICK_REFERENCE.md TECHNICAL_DETAILS.md THE_FIX_EXPLAINED.md V2.1_IMPLEMENTATION_CODE.md WEBSITE_CONNECTIVITY_QUICK_REFERENCE.md",
			"isBackground": false,
			"group": "none"
		},
		{
			"label": "Kill Port 8082",
			"type": "shell",
			"command": "lsof -ti:8082 | xargs kill -9 || true; pkill -f \"Google Chrome\" || true; rm -rf .chrome-dev-profile",
			"presentation": {
				"reveal": "silent",
				"panel": "shared"
			}
		},
		{
			"label": "Git Commit Fix",
			"type": "shell",
			"command": "git commit -m \"Fix pub.dev platform support analysis by removing direct platform package imports\"",
			"isBackground": false,
			"group": "none"
		},
		{
			"label": "Git Commit Fix",
			"type": "shell",
			"command": "git commit -m \"Fix pub.dev platform support analysis by removing direct platform package imports\"",
			"isBackground": false,
			"group": "none"
		},
		{
			"label": "Commit v2.0.6",
			"type": "shell",
			"command": "git add . && git commit -m \"v2.0.6: Add federated plugin configuration for proper pub.dev platform detection\""
		}
	]
}